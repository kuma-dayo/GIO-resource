                    ],
                },
                "Blank":{},
                "Itto_Symbol_Eff_01":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_Weapon_Kunwu_Stack_01",  //临时特效
                        },
                    ],
                },
                "Itto_Symbol_Eff_02":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_Weapon_Kunwu_Stack_02",  //临时特效
                        },
                    ],
                },
                "Itto_Symbol_Eff_03":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_Weapon_Kunwu_Stack_03",  //临时特效
                        },
                    ],
                },
                "Itto_Symbol_Eff_04":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_Weapon_Kunwu_Stack_04",  //临时特效
                        },
                    ],
                },
                "Itto_Symbol_Eff_05":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_Weapon_Kunwu_Stack_05",  //临时特效
                        },
                    ],
                },
                "Avatar_Itto_Calculate_ExtraCombo":  //计算特殊重击打击次数，用于判断何时进最后一击
                {
                    "duration": 0.1,
                    "stacking": "Multiple",
                    "onAdded":
                    [
                        {
                            "$type": "AddGlobalValue",
                            "key": "_ABILITY_Itto_SpecialExtra_Combo",
                            "value": 1,
                            "useLimitRange": true,
                            "minValue": 0,
                            "maxValue": 10,
                            "doOffStage": true,
                        },
                    ],
                },
                "Avatar_Itto_Calculate_ClearExtraCombo":  //退出特殊重击循环时清空Combo段数
                {
                    "onRemoved":
                    [
                        {
                            "$type": "SetGlobalValue",
                            "key": "_ABILITY_Itto_SpecialExtra_Combo",
                            "value": 0,
                            "doOffStage": true,
                        },
                    ],
                    "onAvatarOut":
                    [
                        {
                            "$type": "SetGlobalValue",
                            "key": "_ABILITY_Itto_SpecialExtra_Combo",
                            "value": 0,
                            "doOffStage": true,
                        },
                    ],
                },
            },
        },
    },

