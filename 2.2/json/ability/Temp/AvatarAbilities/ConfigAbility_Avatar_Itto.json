//
//  攻击印记管理中枢
//
////////////////////////////////////////////////////////////////
    {
        "Default":
        {
            "$type": "ConfigAbility",
            "abilityName": "Avatar_Itto_Symbol_Handler",
            "onAdded":
            [
                {
                    "$type": "AttachModifier",
                    "modifierName": "Avatar_Itto_ComboAddSymbol_Handler",
                },
                {
                    "$type": "AttachModifier",
                    "modifierName": "Avatar_Itto_ElementalArtAddSymbol_Handler",
                },
                {
                    "$type": "AttachModifier",
                    "modifierName": "Avatar_Itto_SymbolEffect_Handler",
                },
            ],
            "abilityMixins":
            [
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": [ "ExtraAttack1" ],
                    "modifierName": "Avatar_Itto_Calculate_ExtraCombo",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": [ "ExtraAttack2" ],
                    "modifierName": "Avatar_Itto_Calculate_ExtraCombo",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": [ "ExtraAttack3" ],
                    "modifierName": "Avatar_Itto_Calculate_ExtraCombo",
                },
                {
                    "$type": "AttachToAnimatorStateIDMixin",
                    "stateIDs":
                    [
                        "ExtraAttack1",
                        "ExtraAttack2",
                        "ExtraAttack3",
                    ],
                    "modifierName": "Avatar_Itto_Calculate_ClearExtraCombo",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": [ "ExtraAttack1", "ExtraAttack2", "ExtraAttack3"],
                    "modifierName": "Avatar_Itto_ExtraAttack_ComboSpeed",
                },
                {
                    "$type": "AttachToNormalizedTimeMixin", 
                    "stateID": "ExtraAttack1_BS",
                    "normalizeStart": 0.3,
                    "normalizeEnd": 1,
                    "modifierName": "Avatar_Itto_SpecialExtra_TipEffect",
                },
            ],
            "modifiers":
            {
                "Avatar_Itto_ComboAddSymbol_Handler":  //普通攻击增添印记
                {
                    "onAttackLanded": 
                    [
                        {
                            "$type" : "Predicated",     //第四击增添2枚
                            "target" : "Target",
                            "targetPredicates":
                            [
                                {
                                    "$type" : "ByAttackTags",
                                    "attackTags" : ["NormalAttack_04"],
                                },
                                {
                                    "$type" : "ByEntityTypes",
                                    "entityTypes" : ["Monster"],
                                },
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ANIMAL_ENTITY_TYPE",
                                    "value": 0,
                                    "compareType": "NoneOrEqual",
                                },
                            ],
                            "successActions":
                            [
                                {
                                    "$type": "ApplyModifier",
                                    "modifierName": "Avatar_Itto_LastNormalAttack_AddSymbol",
                                    "target" : "Self",
                                },
                            ],
                        },
                        {
                            "$type" : "Predicated",    //大招状态前三击各增添1枚
                            "target" : "Target",
                            "targetPredicates":
                            [
                                {
                                    "$type" : "ByAttackTags",
                                    "attackTags" : ["NormalAttack_01", "NormalAttack_02", "NormalAttack_03"],
                                },
                                {
                                    "$type" : "ByEntityTypes",
                                    "entityTypes" : ["Monster"],
                                },
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ANIMAL_ENTITY_TYPE",
                                    "value": 0,
                                    "compareType": "NoneOrEqual",
                                },
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_ElementalBurst_Flag",
                                    "value": 1,
                                    "compareType": "Equal",
                                    "target": "Self",
                                },
                            ],
                            "successActions":
                            [
                                {
                                    "$type": "ApplyModifier",
                                    "modifierName": "Avatar_Itto_ForeNormalAttack_AddSymbol",
                                    "target" : "Self",
                                },
                            ],
                        },
                    ],
                },
                "Avatar_Itto_LastNormalAttack_AddSymbol":  //普攻第四击增添2枚印记
                {
                    "duration": 0.1,
                    "stacking": "Unique",
                    "onAdded":
                    [
                        {
                            "$type": "AddGlobalValue",
                            "key": "_ABILITY_Itto_Symbol",
                            "value": 2,
                            "useLimitRange": true,
                            "minValue": 0,
                            "maxValue": 5,
                            "doOffStage": true,
                        },
                    ],
                },
                "Avatar_Itto_ForeNormalAttack_AddSymbol":  //普攻前三击各增添1枚印记
                {
                    "duration": 0.1,
                    "stacking": "Unique",
                    "onAdded":
                    [
                        {
                            "$type": "AddGlobalValue",
                            "key": "_ABILITY_Itto_Symbol",
                            "value": 1,
                            "useLimitRange": true,
                            "minValue": 0,
                            "maxValue": 5,
                            "doOffStage": true,
                        },
                    ],
                },
                "Avatar_Itto_ElementalArtAddSymbol_Handler":  //元素战技增添印记
                {
                    "onAttackLanded":
                    [
                        {
                            "$type" : "Predicated",
                            "target" : "Target",
                            "targetPredicates":
                            [
                                {
                                    "$type" : "ByAttackTags",
                                    "attackTags" : ["Elemental_Art"],
                                },
                                {
                                    "$type" : "ByEntityTypes",
                                    "entityTypes" : ["Monster"],
                                },
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ANIMAL_ENTITY_TYPE",
                                    "value": 0,
                                    "compareType": "NoneOrEqual",
                                },
                                {
                                    "$type": "ByAttackType",
                                    "attackType": "Melee",   //需要与持续的技能伤害作出区分
                                },
                            ],
                            "successActions":
                            [
                                {
                                    "$type": "ApplyModifier",
                                    "modifierName": "ElementalArt_Add_Symbol",
                                    "target" : "Self",
                                },
                            ],
                        },
                    ],
                },
                "ElementalArt_Add_Symbol":  //元素战技增添印记
                {
                    "duration": 0.2,
                    "stacking": "Unique",
                    "onAdded":
                    [
                        {
                            "$type": "AddGlobalValue",
                            "key": "_ABILITY_Itto_Symbol",
                            "value": 1,
                            "useLimitRange": true,
                            "minValue": 0,
                            "maxValue": 5,
                            "doOffStage": true,
                        },
                    ],
                },
                "Avatar_Itto_ExtraAttack_ComboSpeed":   //特殊重击越打越快，暂时去掉
                {
                    "modifierMixins":
                    [
                        {
                            "$type": "AttachModifierToSelfGlobalValueMixin",
                            "globalValueKey": "_ABILITY_Itto_SpecialExtra_Combo",
                            "valueSteps": [-0.5, 1.5, 2.5, 3.5, 4.5, 100],
                            "modifierNameSteps":
                            [
                                "Blank",
                                "Itto_ExtraAttack_ComboSpeed_01",
                                "Itto_ExtraAttack_ComboSpeed_02",
                                "Itto_ExtraAttack_ComboSpeed_03",
                                "Itto_ExtraAttack_ComboSpeed_04",
                            ],
                        },
                    ],
                },
                "Itto_ExtraAttack_ComboSpeed_01":
                {
                    "isLimitedProperties": true,
                    "properties":
                    {
                        "Animator_AttackSpeedRatio": 0.05,
                    },
                },
                "Itto_ExtraAttack_ComboSpeed_02":
                {
                    "isLimitedProperties": true,
                    "properties":
                    {
                        "Animator_AttackSpeedRatio": 0.1,
                    },
                },
                "Itto_ExtraAttack_ComboSpeed_03":
                {
                    "isLimitedProperties": true,
                    "properties":
                    {
                        "Animator_AttackSpeedRatio": 0.15,
                    },
                },
                "Itto_ExtraAttack_ComboSpeed_04":
                {
                    "isLimitedProperties": true,
                    "properties":
                    {
                        "Animator_AttackSpeedRatio": 0.2,
                    },
                },
                "Avatar_Itto_SpecialExtra_TipEffect":  //进入特殊重击的提示效果（隐身+特效）
                {
                    "onAdded":
                    [
                        {
                            "$type": "FireEffect",
                            "effectPattern": "Eff_Avatar_Male_Claymore_Itto_ExtraAttack_TipEffect_01",
                        },
                        {
                            "$type": "FireEffect",
                            "effectPattern": "Eff_Avatar_Male_Claymore_Itto_ExtraAttack_TipEffect_02",
                        },
                        {
                            "$type": "TriggerSetVisible",
                            "visible": false,
                        },
                        {
                            "$type": "TriggerSetPassThrough",
                            "passThrough": true,
                        },
                    ],
                    "onRemoved":
                    [
                        {
                            "$type": "TriggerSetVisible",
                            "doOffStage": true,
                            "canBeHandledOnRecover": true,
                            "visible": true,
                        },
                        {
                            "$type": "TriggerSetPassThrough",
                            "passThrough": false,
                            "doOffStage": true,
                            "canBeHandledOnRecover": true,
                        },
                    ],
                },
                "Avatar_Itto_SymbolEffect_Handler":  //印记特效处理中枢
                {
                    "modifierMixins":
                    [
                        {
                            "$type": "AttachModifierToSelfGlobalValueMixin",
                            "globalValueKey": "_ABILITY_Itto_Symbol",
                            "valueSteps": [ -0.5, 0.5, 1.5, 2.5, 3.5, 4.5, 5.5 ],
                            "removeAppliedModifier": true,
                            "modifierNameSteps": 
                            [
                                "Blank", 
                                "Itto_Symbol_Eff_01", 
                                "Itto_Symbol_Eff_02", 
                                "Itto_Symbol_Eff_03",
                                "Itto_Symbol_Eff_04",
                                "Itto_Symbol_Eff_05",
