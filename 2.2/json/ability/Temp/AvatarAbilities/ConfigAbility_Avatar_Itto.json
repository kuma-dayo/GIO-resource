    //Gadget_41057001  跳板的弹跳效果(测试_主动弹跳)
    {
        "Default":
        {
            "$type": "ConfigAbility",
            "abilityName": "Avatar_Itto_Gadget_GangPlank",
            "onAdded":
            [
                {
                    "$type": "AttachModifier",
                    "modifierName": "GangPlank_RockDurability_Handler",
                },
            ],
            "onAbilityStart":  //与创生物碰撞进的Trigger
            [
                // {
                //     "$type": "Predicated",
                //     "target": "Target",
                //     "targetPredicates":
                //     [
                //         {
                //             "$type": "ByEntityTypes",
                //             "entityTypes": ["Avatar"],
                //         },
                //     ],
                //     "successActions":
                //     [
                //         {
                //             "$type": "ApplyModifier",
                //             "modifierName": "UNIQUE_Itto_GangPlank_RocketJump",
                //             "target": "Target",
                //         },
                //     ],
                // },
            ],
            "modifiers":
            {
                "GangPlank_RockDurability_Handler":  //元素含量处理器
                {
                    "elementType":"Rock",
                    "elementDurability":100,
                    "onBeingHit":
                    [
                        {
                            "$type":"ApplyModifier",
                            "modifierName":"GangPlank_Recover_RockDurability",
                        },
                    ],
                    "onRemoved":
                    [
                        {
                            "$type": "KillSelf",
                            "hideEntity": true,
                        },
                    ],
                },
                "GangPlank_Recover_RockDurability":   //每次受到攻击会慢慢回复
                {
                    "duration":0.5,
                    "onAdded":
                    [
                        {
                            "$type" : "AddElementDurability",
                            "value" : 100,
                            "modifierName":"GangPlank_RockDurability_Handler",
                            "elementType" : "Rock",
                            "useLimitRange": true,
                            "maxValue": 100.0,
                        },
                    ],
                },
                "UNIQUE_Itto_GangPlank_RocketJump":  //跳板对目标施加火箭跳_Avatar上加了个跳高Buff
                {
                    "isUnique": true,
                    "stacking": "Refresh",
                    "duration": 0.3,
                    "onAdded":
                    [
                        {
                            "$type": "EnableRocketJump",
                            "canBeHandledOnRecover": true,
                            "type": "XiaoUlt",
                            "enable": true,
                            "extention":
                            {
                                "xzMultiplier": 2.0,
                                "yMultiplier": 2.3,
                            },
                        },
                    ],
                    "onRemoved":
                    [
                        {
                            "$type": "EnableRocketJump",
                            "canBeHandledOnRecover": true,
                            "type": "XiaoUlt",
                            "enable": false,
                            "doOffStage": true,
                        },
                    ],
                    "properties":
                    {
                        "Actor_FallingDamageRatio": -1,   //起跳瞬间的撞击保护，以免跳起来被撞死
                    },
                    "modifierMixins":
                    [
                        {
                            "$type": "AttachToStateIDMixin",
                            "stateIDs": ["Jump"],
                            "modifierName": "RocketJump_Effect",
                        },
                    ],
                },
                "RocketJump_Effect":  //跳板火箭跳时的特效表现
                {
                    "duration": 0.3,
                    "onAdded":
                    [
                        {
                            "$type": "FireEffect",  
                            "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Smash",  //临时资源
                            "target": "Self",
                            "born":
                            {
                                "$type": "ConfigBornBySelf",
                                "offset":
                                {
                                    "x": 0,
                                    "y": 0.4,
                                    "z": 0,
                                },
                            },
                        },
                    ],
                },
            },
        },
    },

