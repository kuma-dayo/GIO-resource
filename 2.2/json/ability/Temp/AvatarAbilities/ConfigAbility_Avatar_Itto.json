[
///////////////////////////////////////////////////////////////
//
//  普攻和重击
//
////////////////////////////////////////////////////////////////
    {
        "Default":
        {
            "$type": "ConfigAbility",
            "abilityName": "Avatar_Itto_ExtraAttack",
            "abilityMixins":
            [
                {
                    "$type": "ButtonHoldChargeMixin",
                    "skillID": 10571,
                    "chargeTime": 0.12,
                    "secondChargeTime": 0.08,  //这个是临界值 否则容易误触
                    "OnBeginUncharged":
                    [
                        {
                            "$type": "SetAnimatorTrigger",
                            "triggerID": "DoSkill",
                        },
                        {
                            "$type": "SetAnimatorInt",
                            "intID": "SkillID",
                            "value": 1,
                        },
                        {
                            "$type": "SetAnimatorBool",
                            "boolID": "IsCharging",
                            "value": false,
                        },
                        {
                            "$type": "SetAnimatorTrigger",
                            "triggerID": "PRIVATE_DoSpecialExtra",  //特殊重击循环
                            "predicates":
                            [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_Symbol",
                                    "value": 1,
                                    "compareType": "MoreThan",
                                },
                            ],
                        },
                        {
                            "$type": "SetAnimatorTrigger",
                            "triggerID": "PRIVATE_DoLastSpecialExtra",  //特殊重击最后一段
                            "predicates":
                            [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_Symbol",
                                    "value": 1,
                                    "compareType": "Equal",
                                },
                            ],
                        },
                    ],
                    "OnReleaseUncharged":
                    [
                    ],
                    "OnBeginCharged":
                    [
                        {
                            "$type": "SetAnimatorBool",
                            "boolID": "IsCharging",
                            "value": true,
                        },
                    ],
                    "OnReleaseCharged":
                    [
                        {
                            "$type": "SetAnimatorBool",
                            "boolID": "IsCharging",
                            "value": false,
                        },
                        {
                            "$type": "SetAnimatorInt",
                            "intID": "SkillID",
                            "value": 1,
                        },
                    ],
                    "OnBeginSecondCharged":   //真正的蓄力进入判定点 chargeTime+secondChargeTime
                    [
                        {
                            "$type": "SetAnimatorBool",
                            "boolID": "IsCharging",
                            "value": false,
                        },
                        {
                            "$type": "Predicated",
                            "targetPredicates":
                            [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_Symbol",
                                    "value": 0,
                                    "compareType": "MoreThan",
                                },
                            ],
                            "successActions":   //有印记释放特殊重击
                            [
                                {
                                    "$type": "SetAnimatorTrigger",
                                    "triggerID": "DoSkill",
                                },
                                {
                                    "$type": "SetAnimatorInt",
                                    "intID": "SkillID",
                                    "value": 7,
                                },
                            ],
                            "failActions":    //无印记释放普通重击（消耗体力）
                            [
                                {
                                    "$type": "SetAnimatorTrigger",
                                    "triggerID": "DoSkill",
                                    "predicates":
                                    [
                                        {
                                            "$type": "ByLocalAvatarStamina",
                                            "stamina": "%ExtraAttack_Claymore_Stamina_Cost+%Flexible_Stamina",
                                            "logic": "GreaterOrEqual",
                                        },
                                    ],
                                },
                                {
                                    "$type": "SetAnimatorInt",
                                    "intID": "SkillID",
                                    "value": 6,
                                    "predicates":
                                    [
                                        {
                                            "$type": "ByLocalAvatarStamina",
                                            "stamina": "%ExtraAttack_Claymore_Stamina_Cost+%Flexible_Stamina",
                                            "logic": "GreaterOrEqual",
                                        },
                                    ],
                                },
                            ],
                        },
                    ],
                    "chargeStateIDs":
                    [
                        "Attack01_BS", "Attack01", "Attack02", "Attack03", "Attack04", "Run",
                    ],
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": ["ExtraAttack1_BS"],  //这个ExtraAttack_BS可能有问题，因为其他人没这个State
                    "modifierName": "ExtraAttack1_DoLastExtra",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": ["ExtraAttack"],
                    "modifierName": "ExtraAttack_StaminaCost",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": ["ExtraAttack1"],
                    "modifierName": "ExtraAttack_SymbolCost",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": ["ExtraAttack2"],
                    "modifierName": "ExtraAttack_SymbolCost",
                },
                {
                    "$type": "AttachToStateIDMixin",
                    "stateIDs": ["ExtraAttack3"],
                    "modifierName": "ExtraAttack_SymbolCost",
                },
                {
                    "$type": "AttachToNormalizedTimeMixin",
                    "stateID": "ExtraAttack1",
                    "modifierName": "ExtraAttack_ClearTrigger",
                    "normalizeStart": 0.0, 
                    "normalizeEnd": 0.2,
                },
                {
                    "$type": "AttachToNormalizedTimeMixin",
                    "stateID": "ExtraAttack2",
                    "modifierName": "ExtraAttack_ClearTrigger",
                    "normalizeStart": 0.0, 
                    "normalizeEnd": 0.2,
                },
                {
                    "$type": "AttachToNormalizedTimeMixin",
                    "stateID": "Attack04",
                    "modifierName": "NormalAttack_04_Effect",
                    "normalizeStart": 0.042, 
                    "normalizeEnd": 1.0,
                },
            ],
            "modifiers":
            {
                "Blank":{},
                "ExtraAttack1_DoLastExtra":   //进入特殊重击BS时根据当前印记数量决定是否使出最终一击
                {
                    "duration": 0.2,
                    "onAdded":
                    [
                        {
                            "$type": "SetAnimatorTrigger",
                            "triggerID": "PRIVATE_DoLastSpecialExtra",
                            "predicates":
                            [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_Symbol",
                                    "value": 1,
                                    "compareType": "Equal",
                                },
                            ],
                        },
                    ],
                },
                "ExtraAttack_StaminaCost":   //普通重击消耗体力
                {
                    "duration": 0.2,
                    "onAdded":
                    [
                        {
                            "$type" : "AvatarSkillStart",
                            "skillID" : 10001,  //临时填这个值
                            "costStaminaRatio":"%ExtraAttack_Claymore_Stamina_Cost",
                        },
                    ],
                },
                "ExtraAttack_SymbolCost":    //特殊重击消耗印记
                {
                    "duration": 0.2,
                    "stacking": "Multiple",
                    "onAdded":
                    [
                        {
                            "$type": "Predicated",
                            "targetPredicates":
                            [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_ABILITY_Itto_ElementalBurst_Flag",
                                    "value": 1,
                                    "compareType": "Equal",
                                },
                            ],
                            "successActions":
                            [
                                {
                                    "$type": "Randomed",
                                    "chance": 0.5,
                                    "successActions":
                                    [
                                        {
                                            "$type": "AddGlobalValue",
                                            "key": "_ABILITY_Itto_Symbol",
                                            "value": -1,
                                            "useLimitRange": true,
                                            "minValue": 0,
                                            "maxValue": 5,
                                            "doOffStage": true,
                                        },
                                    ],
                                },
                            ],
                            "failActions":
                            [
                                {
                                    "$type": "AddGlobalValue",
                                    "key": "_ABILITY_Itto_Symbol",
                                    "value": -1,
                                    "useLimitRange": true,
                                    "minValue": 0,
                                    "maxValue": 5,
                                    "doOffStage": true,
                                },
                            ],
                        },
                    ],
                },
                "ExtraAttack_ClearTrigger":  //特殊重击前段固定时刻ResetTrigger
                {
                    "onRemoved":
                    [
                        {
                            "$type": "ResetAnimatorTrigger",
                            "triggerID": "PRIVATE_DoSpecialExtra",       //特殊重击Trigger
                            "doOffStage": true,
                        },
                        {
                            "$type": "ResetAnimatorTrigger",
                            "triggerID": "PRIVATE_DoLastSpecialExtra",   //特殊重击最后一击Trigger
                            "doOffStage": true,
                        },
                    ],
                },
                "NormalAttack_04_Effect":    //普攻第四击踢刀特效
                {
                    "onAdded":
                    [
                        {
                            "$type": "FireEffect",
                            "effectPattern": "Eff_Avatar_Male_Claymore_Itto_Attack_04_Cast",
                        },
                        {
                            "$type": "FireEffect",
                            "effectPattern": "Eff_Avatar_Male_Claymore_Itto_Attack_04_Weapon",
                            "otherTargets":
                            {
                                "$type": "SelectTargetsByEquipParts",
                                "equipPartNames" : ["equip01", "equip02", "sheathPoint"],
                            },
                        },
                    ],
                },
            },
            "abilitySpecials":
            {
                "ExtraAttack_Claymore_Stamina_Cost": 0,
                "Flexible_Stamina": -2,  //照顾延迟放宽一些判定条件
            },  
        },
    },

